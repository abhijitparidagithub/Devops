public with sharing class Calculator {
    public Calculator() {

    }
    public integer add(integer a, integer b)
    {
        return a+b;
    }
    public integer add(integer a, integer b, interger c)
    {
        return a+b+c;
    }
    public integer add(integer a, integer b, interger d)
    {
        return a+b+d;
    }
}
